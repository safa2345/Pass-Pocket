<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" styleClass="main" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mir.passpocket.Controller.NoteVaultController">

   <children>
      <Label id="title" layoutX="512.0" layoutY="14.0" prefHeight="62.0" prefWidth="230.0" text="All Notes" />
      <ListView fx:id="listNote" layoutX="86.0" layoutY="93.0" prefHeight="614.0" prefWidth="1027.0" />
      <Button fx:id="backBtn" layoutX="1050.0" layoutY="727.0" mnemonicParsing="false" text="Back" />
      <Button fx:id="editBtn" layoutX="589.0" layoutY="721.0" prefHeight="25.0" prefWidth="60.0">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icon/edit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="deleteBtn" layoutX="702.0" layoutY="721.0" prefHeight="25.0" prefWidth="60.0">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icon/trash.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="addBtn" layoutX="480.0" layoutY="721.0" prefHeight="25.0" prefWidth="60.0">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icon/add.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>

    <stylesheets>
        <URL value="@../css/noteVaultStyle.css" />
        <URL value="@../css/style.css" />
    </stylesheets>

</AnchorPane>
